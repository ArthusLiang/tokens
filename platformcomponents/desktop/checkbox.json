{
  "checkbox": {
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows%2BWeb?node-id=4371%3A10197",
    "checked":{
      "#normal":{
        "background": "@theme-control-active-normal",
        "text": "@theme-text-primary-normal",
        "border": "@theme-button-border-normal",
        "icon": "@theme-text-primary-inverted"
      },
      "#hovered":{
        "background": "@theme-control-active-hover",
        "text": "@theme-text-primary-normal",
        "border": "@theme-button-border-normal",
        "icon": "@theme-text-primary-inverted"
      },
      "#pressed":{
        "background": "@theme-control-active-pressed",
        "text": "@theme-text-primary-normal",
        "border": "@theme-button-border-normal",
        "icon": "@theme-text-primary-inverted"
      },
      "#disabled":{
        "background": "@theme-control-active-disabled",
        "text": "@theme-text-primary-disabled",
        "border": "@theme-button-border-normal",
        "icon": "@theme-text-primary-disabled"
      }
    },
    "unchecked":{
      "#normal":{
        "background": "@theme-background-secondary-normal",
        "text": "@theme-text-primary-normal",
        "border": "@theme-control-inactive-normal"
      },
      "#hovered":{
        "background": "@theme-background-secondary-hover",
        "text": "@theme-text-primary-normal",
        "border": "@theme-control-inactive-hover"
      },
      "#pressed":{
        "background": "@theme-background-secondary-hover",
        "text": "@theme-text-primary-normal",
        "border": "@theme-control-inactive-pressed"
      },
      "#disabled":{
        "background": "@theme-background-secondary-hover",
        "text": "@theme-text-primary-disabled",
        "border": "@theme-control-inactive-disabled"
      }
    }
  }
}
