{
  "sharewindow": {
    "comment": "Applied to share window",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows%2BWeb?node-id=9532%3A17790",
    "preview": {
      "#normal": {
        "active": "@theme-common-control-shareContent-active",
        "inactive": "@theme-common-control-shareContent-inactive"
      }
    },
    "border": {
      "#normal": {
        "selected": "@theme-common-control-shareContent-selected",
        "active": "@theme-common-control-shareContent-active",
        "inactive": "@theme-common-control-shareContent-inactive"
      }
    },
    "control": {
      "text": {
        "primary": "@theme-common-text-white",
        "inactive": "@theme-common-text-white"
      },
      "dragger": {
        "primary": "@theme-common-outline-button-normal",
        "inactive": "@theme-common-outline-button-normal"
      },
      "button": {
        "active": {
          "#normal": {
            "background": "@theme-common-button-secondary-active-normal",
            "text": "@theme-common-text-white",
            "border": "@theme-common-outline-button-normal"
          },
          "#hovered": {
            "background": "@theme-common-button-secondary-active-hover",
            "text": "@theme-common-text-white",
            "border": "@theme-common-outline-button-normal"
          },
          "#pressed": {
            "background": "@theme-common-button-secondary-active-pressed",
            "text": "@theme-common-text-white",
            "border": "@theme-common-outline-button-normal"
          },
          "#disabled": {
            "background": "@theme-common-button-secondary-disabled",
            "text": "@theme-common-text-disabled",
            "border": "@theme-common-outline-disabled-normal"
          }
        }
      }
    },
    "viewwindow": {
      "background": {
        "#normal": "@theme-background-primary-active",
        "#disabled": "@theme-background-primary-disabled"
      },
      "text": {
        "#normal": "@theme-text-primary-normal",
        "#disabled": "@theme-text-primary-disabled"
      },
      "border": {
        "notSharing": {
          "#normal": "none",
          "#hovered": "@theme-outline-input-normal",
          "#active": "@theme-outline-theme-normal",
          "#disabled": "none"
        },
        "sharing": {
          "#normal": "@theme-outline-share-active"
        }
      }
    }
  }
}
