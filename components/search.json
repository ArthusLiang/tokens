{
  "search": {
    "comment": "Applied to search input box. Background colours are different for mobile vs web/desktop, so appear in platformcomponents directory",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=4347%3A244",
    "placeholder-text": "@theme-text-secondary-normal",
    "#normal": {
      "text": "@theme-text-primary-normal",
      "border": "@theme-outline-input-normal",
      "searchicon": "@theme-text-primary-normal"
    },
    "#hovered": {
      "text": "@theme-text-primary-normal",
      "border": "@theme-outline-input-normal",
      "searchicon": "@theme-text-primary-normal"
    },
    "#active": {
      "text": "@theme-text-primary-normal",
      "border": "@theme-outline-input-active",
      "searchicon": "@theme-text-primary-normal"
    },
    "#disabled": {
      "text": "@theme-text-primary-disabled",
      "border": "none",
      "searchicon": "@theme-text-primary-disabled"
    }
  }
}
